<p id="notice"><%= notice %></p>

<h1>Tpersonas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tpersonas.each do |tpersona| %>
      <tr>
        <td><%= tpersona.tipo %></td>
        <td><%= link_to 'Show', tpersona %></td>
        <td><%= link_to 'Edit', edit_tpersona_path(tpersona) %></td>
        <td><%= link_to 'Destroy', tpersona, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tpersona', new_tpersona_path %>
